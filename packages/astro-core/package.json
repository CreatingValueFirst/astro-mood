{
  "name": "@astro-mood/astro-core",
  "version": "0.1.0",
  "description": "Core astrological calculation engine for AstroMood",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "astrology",
    "astronomy",
    "ephemeris",
    "natal-chart",
    "transits"
  ],
  "author": "AstroMood Team",
  "license": "MIT",
  "dependencies": {
    "astronomy-engine": "^2.1.19",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
