{
  "name": "astro-mood-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "build": "npm run build:core && npm run build:web",
    "build:core": "npm run build --workspace=@astro-mood/astro-core",
    "build:web": "npm run build --workspace=web",
    "build:all": "npm run build:core && npm run build:web",
    "start": "cd apps/web && npm run start"
  },
  "description": "AstroMood - Your Cosmic Mood Companion",
  "repository": {
    "type": "git",
    "url": "https://github.com/CreatingValueFirst/astro-mood.git"
  },
  "devDependencies": {
    "vercel": "^50.4.5"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.1.13"
  }
}
